FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    location        "system";
    object          decomposeParDict;
}

numberOfSubdomains 4;

method          simple;

engineScotchCoeffs
{
    slidingPatchPairs ( );
    expandSliding   false;
}

simpleCoeffs
{
    n               ( 1 1 4 );
    delta           0.001;
}

hierarchicalCoeffs
{
    n               ( 1 2 2 );
    delta           0.001;
    order           xyz;
}

manualCoeffs
{
    dataFile        ".././constant/cellDist";
}


